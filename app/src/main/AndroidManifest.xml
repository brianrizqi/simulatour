<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.unej.ilkom.simulatour">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".Networks.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Sewa" />
        <activity android:name=".Activities.SimulatorDetail" />
        <activity android:name=".Activities.About" />
        <activity android:name=".Activities.Wisata" />
        <activity android:name=".Activities.Penginapan" />
        <activity android:name=".Activities.Transportasi" />
        <activity android:name=".Activities.Makanan" />
        <activity android:name=".Activities.WisataDetail" />
        <activity android:name=".Activities.MakananDetail" />
        <activity android:name=".Activities.SewaDetail" />
        <activity android:name=".Activities.TransportasiDetail" />
        <activity android:name=".Activities.PenginapanDetail"></activity>
    </application>

</manifest>